<!DOCTYPE html>
<html>
  <head>
    <title>AstrologyFacebook</title>
    <%= stylesheet_link_tag "application", 'facebox' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tag %>
    <%= yield(:head) %>
  <!--script src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js" type="text/javascript"></script-->
  <%= javascript_include_tag 'facebox' %>
  <script type="text/javascript">
    jQuery(document).ready(function($) {
      $('a[rel*=facebox]').facebox({
        loadingImage : '/images/loading.gif',
        closeImage   : '/images/closelabel.png'
      })
    })
  </script>
  </head>

  <body>
	<div id="fb-root"></div>
	<script>(function(d, s, id) {
		var js, fjs = d.getElementsByTagName(s)[0];
		if (d.getElementById(id)) return;
		js = d.createElement(s); js.id = id;
		js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=389241711100927";
		fjs.parentNode.insertBefore(js, fjs);
	}(document, 'script', 'facebook-jssdk'));</script>

  <div class='page_title'>
    <img src='https://graph.facebook.com/237278599679568/picture'>
    <h3>Astrology Horoscope</h3>
  </div>
  <br clear='all'/>

  <%= yield %>

  </div>
</body>
</html>



